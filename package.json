		"@testing-library/svelte": "^5.2.7",
		"@types/node": "^22.13.11",
		"@vitest/ui": "^2.1.8",
		"autoprefixer": "^10.4.20",
		"daisyui": "^5.0.0-beta.1",
		"jsdom": "^26.0.0",
		"postcss": "^8",
